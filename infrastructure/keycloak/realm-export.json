{
  "realm": "delifhery",
  "enabled": true,
  "registrationAllowed": false,
  "rememberMe": true,
  "resetPasswordAllowed": true,
  "internationalizationEnabled": false,
  "clients": [
    {
      "clientId": "delifhery-api",
      "name": "DeliFHery Protected API",
      "rootUrl": "",
      "surrogateAuthRequired": false,
      "enabled": true,
      "bearerOnly": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "defaultRoles": [
        "delivery-dispatcher"
      ],
      "attributes": {
        "access.token.lifespan": "900"
      }
    },
    {
      "clientId": "delifhery-web",
      "name": "DeliFHery SPA",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "*"
      ],
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "defaultClientScopes": [
        "profile",
        "email"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "delivery-dispatcher",
        "description": "Users allowed to read and manage deliveries"
      }
    ]
  },
  "users": [
    {
      "username": "dispatcher",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Delivery",
      "lastName": "Dispatcher",
      "email": "dispatcher@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "ChangeMe123!",
          "temporary": false
        }
      ],
      "realmRoles": [
        "delivery-dispatcher"
      ]
    }
  ]
}
